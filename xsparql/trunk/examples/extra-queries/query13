prefix : <http://www.example.org>
prefix foaf: <http://xmlns.com/foaf/0.1/>
let $y := "<http://www.example.org/knownPerson>"
for * from <http://www.polleres.net/foaf.rdf>
where {$s foaf:knows $x}
return <aa>{
     for * from <http://www.polleres.net/foaf.rdf> 
     where {$x foaf:knows $y}
    return <b>{$y}</b>
 }</aa>

