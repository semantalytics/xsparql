# bindings with two variables and two sets of values

prefix : <http://example.org/> 
prefix foaf: <http://xmlns.com/foaf/0.1/> 
<results>
{for $s $o1 $o2
from <src/main/resources/xsparql/testcases-dawg-sparql-1.1/bindings/data07.ttl> where{
  $s $p1 $o1 
  optional { $s foaf:knows $o2 }
} values ($o2) {
 (:b)
}

return 
 <result>
	<binding name="o2">{$o2}</binding>
	<binding name="o1">{$o1}</binding>
	<binding name="s">{$s}</binding>
</result>}
</results>
