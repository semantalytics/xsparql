# bindings with two variables and one set of values

prefix : <http://example.org/> 

<results>
{for $s $o1 $o2
from <src/main/resources/xsparql/testcases-dawg-sparql-1.1/bindings/data03.ttl> where{
  $s $p1 $o1 .
  $s $p2 $o2 .
} values ($o1 $o2) {
 ("alan" "alan@example.org")
}

return 
 <result>
	<binding name="o2">{$o2}</binding>
	<binding name="o1">{$o1}</binding>
	<binding name="s">{$s}</binding>
</result>}
</results>
